fetch('/data/council-test.geojson')
  .then(r => r.json())
  .then(fc => fc.features.slice(0,5).map(f => ({
    name: f.properties?.name || f.properties?.NAME || f.properties?.lad23nm,
    coords: Array.isArray(f.geometry?.coordinates)
      ? f.geometry.coordinates[0]?.length // rough count of ring points
      : 0
  })))
  .then(console.log);
// Expect coords >> 5 (dozens/hundreds), not 4 or 5.
